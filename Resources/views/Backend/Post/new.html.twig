{% extends 'QbBlogBundle::Backend\\layout.html.twig' %}

{% block title %}Post creation - {{ parent() }}{% endblock %}

{% block qb_blog_body %}
    <h1>Post creation</h1>
    <form action="{{ path('qb_blog_backend_post_new') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <input type="submit" />
    </form>
{% endblock %}
